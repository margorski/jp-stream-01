inline u2 {
    // XOR z 0xff żeby zanegować
    a^0xff c- a+1
}

// x, y - trybu znakowego
inline get_tile {
    ptr_a=a=&<screen
	ptr_a+1=a=&>screen
    temp1=y

    a=y != {
        {
            a=ptr_a c- a+40 ptr_a=a a=ptr_a+1 a+0 ptr_a+1=a
            y--
        } !=
    }

    temp2=x
    y=temp2
    a=(ptr_a),y    
    y=temp1
}

// x, y - gracza
func get_tile_from_sprite_coordinates {
    a=x c+ a-48 a>> a>> x=a 
    a=y c+ a-24 a>> a>> a>> y=a
    get_tile
}